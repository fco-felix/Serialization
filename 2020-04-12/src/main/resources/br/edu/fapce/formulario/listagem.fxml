<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.fapce.formulario.ListagemController">
    <children>
      <Label text="Listagem">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tv_Tabela" prefHeight="389.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="Nome" prefWidth="75.0" text="Nome" />
          <TableColumn fx:id="CPF" prefWidth="75.0" text="CPF" />
            <TableColumn fx:id="Nascimento" prefWidth="75.0" text="Nascimento" />
            <TableColumn fx:id="Mae" prefWidth="75.0" text="Nome da MÃ£e" />
        </columns>
      </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="bt_Cadastro" mnemonicParsing="false" onAction="#bt_CadastroOnAction" prefHeight="25.0" prefWidth="100.0" text="Cadastro">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
              <Button fx:id="bt_Menu" onAction="#bt_MenuOnAction" prefHeight="25.0" prefWidth="100.0" text="Menu Principal" />
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
